# AWS Configuration
aws_region  = "ap-southeast-2" # Change to your region
environment = "test"           # or "prod"

# CloudWatch Configuration
log_retention_days = 7 # 7 for test, 30 for prod

# Lambda Configuration
lambda_memory_size = 128 # 128 for test, 256+ for prod
lambda_timeout     = 30

# jmap-service-core integration
# Table name follows pattern: jmap-service-core-data-<env>
jmap_core_table_name = "jmap-service-core-data-test" # or "jmap-service-core-data-prod"

# Plugin version
plugin_version = "1.0.0"
